<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.maheffa.TabulatedOCR.DBManager.TextFormat" table="TextFormat" catalog="TOCR">
        <id name="idTextFormat" type="java.lang.Integer">
            <column name="idTextFormat" />
            <generator class="identity" />
        </id>
        <many-to-one name="format" class="com.maheffa.TabulatedOCR.DBManager.Format" fetch="select">
            <column name="idFormat" />
        </many-to-one>
        <property name="content" type="string">
            <column name="content" length="10000" />
        </property>
    </class>
</hibernate-mapping>
