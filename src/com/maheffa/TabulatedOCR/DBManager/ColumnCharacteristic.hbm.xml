<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.maheffa.TabulatedOCR.DBManager.ColumnCharacteristic" table="ColumnCharacteristic" catalog="TOCR">
        <id name="idColumnCharacteristic" type="java.lang.Integer">
            <column name="idColumnCharacteristic" />
            <generator class="identity" />
        </id>
        <many-to-one name="tableFormat" class="com.maheffa.TabulatedOCR.DBManager.TableFormat" fetch="select">
            <column name="idTableFormat" />
        </many-to-one>
        <property name="position" type="java.lang.Integer">
            <column name="position" />
        </property>
        <property name="name" type="string">
            <column name="name" length="45" />
        </property>
        <property name="type" type="string">
            <column name="type" length="45" />
        </property>
    </class>
</hibernate-mapping>
